# Exemplo de variáveis de ambiente
GCP_PROJECT=ingestaokraken
GOOGLE_APPLICATION_CREDENTIALS=/opt/sisacao/keys/gcp-sa.json
# Tabela de candles diários utilizada pela função get_stock_data
BQ_TABLE=ingestaokraken.cotacao_intraday.cotacao_ohlcv_diario
# Tabelas intraday geradas pelas funções google_finance e intraday_candles
BQ_INTRADAY_RAW=ingestaokraken.cotacao_intraday.cotacao_b3
BQ_INTRADAY_15M=ingestaokraken.cotacao_intraday.candles_intraday_15m
BQ_INTRADAY_1H=ingestaokraken.cotacao_intraday.candles_intraday_1h
GCP_REGION=us-central1
AWS_STATIC_IP=34.194.252.70

# Estratégia de idempotência da carga diária: DELETE_PARTITION_APPEND (default) ou MERGE
BQ_DAILY_LOAD_STRATEGY=MERGE

# Parâmetros dos sinais EOD
SIGNAL_X_PCT=0.02
SIGNAL_TARGET_PCT=0.07
SIGNAL_STOP_PCT=0.07
SIGNAL_HORIZON_DAYS=10
SIGNAL_RANKING_KEY=score_v1
MAX_SIGNALS=5
ALLOW_SELL_SIGNALS=true
ALLOW_EARLY_SIGNAL=false

# Backtest diário
BACKTEST_METRICS_LOOKBACK_DAYS=60
